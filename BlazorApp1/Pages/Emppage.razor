
@page "/fetchdataemp"
<PageTitle>Weather forecast</PageTitle>

@using BlazorApp1.Data
@using BlazorApp1.Models

<h1>Employee List</h1>



@inject Empservice Empservice

@if (forecasts == null)
{
    <p><em>Loading...</em></p>
}
else
{
    <table class="table">
        <thead>
            <tr>
                <th>Name</th>
                <th>Age</th>
                <th>Address</th>
             
            </tr>
        </thead>
        <tbody>
            @foreach (var forecast in forecasts)
            {
                <tr>
                    <td>@forecast.name</td>
                    <td>@forecast.age</td>
                    <td>@forecast.address</td>
              
                </tr>
            }
        </tbody>
    </table>
}


@code {
    private List<SP_selectResult>? forecasts;

    protected override async Task OnInitializedAsync()
    {
        forecasts = await Empservice.SP_selectAsync();
    }

}
